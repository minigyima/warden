{
    "name": "minigyima/warden",
    "description": "A robust and modular access control library for Laravel",
    "type": "library",
    "require": {
        "illuminate/support": ">=9.0",
        "illuminate/cache": ">=9.0",
        "illuminate/database": ">=9.0",
        "illuminate/console": ">=9.0",
        "illuminate/framework": ">=9.0",
        "spatie/laravel-ignition": ">=1.0",
        "php": "^8.1"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Minigyima\\Warden\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Máté Gyimesi",
            "email": "gyim.mate@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Minigyima\\Warden\\Providers\\WardenServiceProvider"
            ],
            "aliases": {
                "Warden": "Minigyima\\Warden\\Facades\\Warden"
            }
        }
    },
    "minimum-stability": "stable"
}
